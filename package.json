{
  "name": "hebe-api",
  "version": "1.0.1",
  "description": "Unofficial library for programmatic access to your Hebe account and your orders",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/cjs/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "clean": "rimraf lib",
    "prebuild": "yarn clean",
    "build": "concurrently \"yarn build:cjs\" \"yarn build:esm\"",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "lint": "eslint --ext=.tsx,.ts .",
    "lint-staged": "lint-staged -c .lintstagedrc.json",
    "docs": "npx typedoc --options typedoc.json",
    "prepack": "yarn build",
    "prepublish": "yarn build",
    "test": "jest"
  },
  "repository": {
    "url":"https://github.com/tomek7667/Hebe-API-js"
  },
  "author": "tomek7667",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.3",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.12",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typescript": "^5.4.3"
  },
  "dependencies": {}
}
